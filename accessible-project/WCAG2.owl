        <Approach_has_OutputResult rdf:resource="http://www.AccessibleOntology.com/GenericOntology.owl#Total_Input_Radio_Elements"/>
        <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Check if every checkbox or radio button is followed by a label immediately after it</hasDescription>
