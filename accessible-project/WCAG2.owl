                            >false</isAutomatic>
                            <Approach_has_OutputResult rdf:resource="http://www.AccessibleOntology.com/GenericOntology.owl#Non_Data_Tables"/>
                            <Approach_belongsTo_Checkpoint rdf:resource="#SuccessCriterion_1.3.1"/>
                            <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                            >A259_checkForDataAndLayoutTables</hasName>
