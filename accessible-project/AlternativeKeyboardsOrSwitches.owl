                <Disability_has_Device>
                  <j.0:Alternative_Keyboard_Or_Switches rdf:ID="Microsoft_Natural_keyboard">
                    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >Microsoft Natural Keyboard</hasName>
