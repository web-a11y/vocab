            <Device_belongsTo_Disability rdf:resource="http://www.AccessibleOntology.com/GenericOntology.owl#Expressive_Language_Disorder"/>
            <Device_belongsTo_Disability rdf:resource="http://www.AccessibleOntology.com/GenericOntology.owl#Tourette_Syndrome"/>
          </j.0:Alternative_Keyboard_Or_Switches>
        </Disability_has_Device>
