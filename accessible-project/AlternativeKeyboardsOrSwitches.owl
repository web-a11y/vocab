                                <Disability_has_Device rdf:resource="#PerfecTouch_101_Keyboard"/>
                                <Disability_has_Device rdf:resource="#PACE_Adjustable_Keyboard"/>
                                <Disability_has_Device rdf:resource="#Microsoft_Natural_keyboard"/>
                                <Disability_has_Device rdf:resource="#Kinesis_Maxim"/>
                              </rdf:Description>
                            </Device_belongsTo_Disability>
                            <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                            >The award winning Kinesis Maxim adjustable ergonomic computer keyboard is the&lt;/br&gt;only ergonomic keyboard licensed to use the Microsoft &amp;quot;combo&amp;quot;&lt;/br&gt;USB/PS2 technology.</hasDescription>
                          </j.0:Alternative_Keyboard_Or_Switches>
                        </Disability_has_Device>
                      </rdf:Description>
                    </Device_belongsTo_Disability>
                    <Device_belongsTo_Disability rdf:resource="http://www.AccessibleOntology.com/GenericOntology.owl#Expressive_Language_Disorder"/>
                    <Device_belongsTo_Disability rdf:resource="http://www.AccessibleOntology.com/GenericOntology.owl#Aspergers_Syndrome"/>
                    <Device_belongsTo_Disability rdf:resource="http://www.AccessibleOntology.com/GenericOntology.owl#Communication_Disability"/>
                    <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >The Microsoft Natural keyboard is an ergonomic keyboard that lets you work more&lt;/br&gt;comfortably. The variety of buttons help you navigate easily and perform&lt;/br&gt;the needed tasks more efficiently.</hasDescription>
                    <hasId rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                    >1</hasId>
                    <Device_belongsTo_Disability rdf:resource="http://www.AccessibleOntology.com/GenericOntology.owl#Autism"/>
                    <Device_belongsTo_Disability rdf:resource="http://www.AccessibleOntology.com/GenericOntology.owl#Tic_Disorders"/>
                  </j.0:Alternative_Keyboard_Or_Switches>
                </Disability_has_Device>
                <Disability_has_Device rdf:resource="#Kinesis_Maxim"/>
              </rdf:Description>
            </Device_belongsTo_Disability>
